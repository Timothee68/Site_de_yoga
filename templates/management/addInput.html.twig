{% extends 'base.html.twig' %}

{% block title %}Ajouter un bénéfices à la liste
{% endblock %}

{% block body %}
	{{ form_start(formAddInput) }}
	{{ form_errors(formAddInput) }}
	<div class="container">
		<div class="row">
			<div class="col-4 mb-4 mt-4">
				{{ form_row(formAddInput.description) }}
			</div>
			<div class="row">
				<div class="col-4 mt-4">
					{{ form_row(formAddInput.submit, { 'label': 'Envoyer' }) }}
				</div>
			</div>
		</div>
	</div>
	{{ form_end(formAddInput) }}
{% endblock %}
